{
    "name": "echoweb",
    "private": true,
    "version": "1.0.0",
    "description": "",
    "main": "package.json",
    "author": "Bo Aanes <boaanes@gmail.com>",
    "license": "UNLICENSED",
    "scripts": {
        "start": "sanity start",
        "build": "sanity build",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "prepare": "husky install"
    },
    "keywords": [
        "sanity"
    ],
    "renovate": {
        "extends": [
            "config:base"
        ],
        "prHourlyLimit": 0,
        "stabilityDays": 3,
        "prCreation": "not-pending",
        "ignoreDeps": [
            "postgres",
            "node"
        ]
    },
    "prettier": {
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
    },
    "dependencies": {
        "@sanity/base": "2.21.5",
        "@sanity/core": "2.21.5",
        "@sanity/default-layout": "2.21.5",
        "@sanity/default-login": "2.21.4",
        "@sanity/desk-tool": "2.21.5",
        "@sanity/vision": "2.21.5",
        "prop-types": "15.7.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "sanity-plugin-markdown": "2.0.3",
        "styled-components": "5.3.3"
    },
    "devDependencies": {
        "@sanity/cli": "2.21.5",
        "husky": "7.0.4",
        "prettier": "2.4.1"
    }
}
